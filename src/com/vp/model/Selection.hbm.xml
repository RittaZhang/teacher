<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="com.vp.model.Selection" table="SELECTION">
		<id name="id" type="int">
			<generator class="native"/>  <!-- 自增长 -->
		</id>
		<property name="optionorder" type="int"></property>
		<property name="groupname" type="java.lang.String"></property>
		<property name="optionvalue" type="java.lang.String"></property>
		<property name="remarks" type="java.lang.String"></property>

	</class>
</hibernate-mapping>