<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="com.vp.model.Manager" table="MANAGER">
		<id name="id" type="int">
			<generator class="native"/>  <!-- 自增长 -->
		</id>
		<property name="account" type="java.lang.String"></property>
		<property name="password" type="java.lang.String"></property>
		<property name="power" type="java.lang.String"></property>

	</class>
</hibernate-mapping>